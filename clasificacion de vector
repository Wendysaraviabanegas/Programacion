
#include "stdafx.h"
#define NMAX 1000
void LEER_VEC(int*A, int n);
void burbuja(int*A, int N);
void imprima_vec(int*A, int N);
int main(void)
{int N, A[NMAX];
printf("\n tama√±o del vector : ");
scanf("%d",&A);
LEER_VEC(A,N);
burbuja(A,N);
imprima_vec(A,N);
	return 0;
}

 void LEER_VEC(int*A, int n)
 {int i;
 for(i=0;i<n;i++)
 {printf("A[%d]: ",i);
 scanf("%f", &A[i]);}
 }
void imprimavec(int*A, int N)
{
	int i;
	for(i=0;i<N;i++)
	{printf("\n A[%d]",i,A[i]);
	}
}
 void burbuja(int*A, int N)
{int i, k, aux;
	{
		for(k=0;k<N-1;k++)
		{
			for(i=0;i<N-1*k;i++)
	{if (A[i]>A[i+1])
	{aux=A[i];
	A[i]=A[i+1];
	A[i+1]=aux;}
			}
		}
	}
 }
 
